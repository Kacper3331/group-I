<h1>Wishlists</h1>
<div>
  <ul>
    <% @wishlists.each do |wishlist| %>
      <li>
        <%= link_to wishlist.name, wishlist %>
        <%= link_to "Edit", edit_wishlist_path(wishlist) %>
        <%= button_to "Delete", wishlist, method: :delete %>
      </li>
    <% end %>
    <li>
      <%= link_to "Create new wishlist", new_wishlist_path %>
    </li>
  </ul>
</div>
